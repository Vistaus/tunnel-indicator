<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="tunnel-indicator@atareao.es">
    <schema path="/es/atareao/tunnel-indicator/" id="es.atareao.tunnel-indicator">
        <key name="tunnels" type="as" >
            <default>["tunnel1|command1", "tunnel2|command2"]</default>
            <summary>List of tunnels definition</summary>
            <description>Set the definition for every tunnel</description>
        </key>
        <key name="checktime" type="i" >
            <default>5</default>
            <summary>Check time</summary>
            <description>Set the check time to test the ssh tunnel status</description>
        </key>
        <key name="darktheme" type="b">
            <default>false</default>
            <summary>Dark theme</summary>
            <description>Enable dark theme otherwise light theme</description>
        </key>
    </schema>
</schemalist>
